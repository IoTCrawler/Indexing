    <!-- This is the tittle for the tab on the browser-->
    <title>{{title}}</title>

    <!-- This is the navigation ** Start's here **-->

    <!--<nav class="navbar navbar-expand-lg navbar-dark">-->
    <nav class="navbar navbar-expand-lg navbar-dark bg-light">

        <a class="navbar-brand" href="/search-engine/">
            <img src="/search-engine/images/IoTCrawler_LogoColor_neg.png" width="170" height="30"
                 alt="">
        </a>

        <!--Button for mobile phones-->
        <button class="navbar-toggler" type="button" data-toggle="collapse"
                data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <!--Button for mobile phones-->

        <!--<div class="navbar collapse navbar-collapse navbar-collapse">-->
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                {{#each this.nav}}
                    <li class="nav-item">
                        <a class="nav-link" href={{this.link}}> |
                            <i class=" {{this.icon}}"></i>
                            {{this.title}}
                            <!--<span class="sr-only">(current)</span>-->
                        </a>
                    </li>
                {{/each}}
            </ul>
            <ul class="navbar-nav" style="color: white">
                <li class="nav-item">
                    <a style="text-align: right;color: #aadaff" href="/signIn"><i class="fa fa-sign-in-alt"></i> Sign in</a> /
                    <a style="text-align: right;color: #aadaff" href="/signUp"><i class="fa fa-user-plus"></i> Sign Up</a>
                </li>
            </ul>

        </div>
    </nav>

    <!-- This is the navigation ** end's here **-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    <!--Chart js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>

</head>
